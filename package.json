    "lint-staged": "^13.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
